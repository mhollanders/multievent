@article{ortiz-santaliestra2013,
  title = {Interspecific and Postmetamorphic Variation in Susceptibility of Three {{North American}} Anurans to {{{\emph{Batrachochytrium}}}}{\emph{ Dendrobatidis}}},
  author = {{Ortiz-Santaliestra}, Manuel E. and Rittenhouse, Tracy A. G. and Cary, Tawnya L. and Karasov, William H.},
  year = {2013},
