@article{carey2006,
  title = {Experimental Exposures of Boreal Toads ({{{\emph{Bufo}}}}{\emph{ Boreas}}) to a Pathogenic Chytrid Fungus ({{{\emph{Batrachochytrium}}}}{\emph{ Dendrobatidis}})},
  author = {Carey, Cynthia and Bruzgul, Judsen E. and Livo, Lauren J. and Walling, Margie L. and Kuehl, Kristin A. and Dixon, Brenner F. and Pessier, Allan P. and Alford, Ross A. and Rogers, Kevin B.},
